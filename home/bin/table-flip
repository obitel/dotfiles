#!/bin/bash
node -e 'function flipString(r){r=r.toLowerCase();for(var l=r.length-1,o="",t=l;t>=0;--t)o+=flipChar(r.charAt(t));return o}function flipChar(r){return"a"==r?"ɐ":"b"==r?"q":"c"==r?"ɔ":"d"==r?"p":"e"==r?"ǝ":"f"==r?"ɟ":"g"==r?"b":"h"==r?"ɥ":"i"==r?"ı":"j"==r?"ظ":"k"==r?"ʞ":"l"==r?"ן":"m"==r?"ɯ":"n"==r?"u":"o"==r?"o":"p"==r?"d":"q"==r?"b":"r"==r?"ɹ":"s"==r?"s":"t"==r?"ʇ":"u"==r?"n":"v"==r?"ʌ":"w"==r?"ʍ":"x"==r?"x":"y"==r?"ʎ":"z"==r?"z":"["==r?"]":"]"==r?"[":"("==r?")":")"==r?"(":"{"==r?"}":"}"==r?"{":"?"==r?"¿":"¿"==r?"?":"!"==r?"¡":"."==r?"˙":"_"==r?"‾":";"==r?"؛":"9"==r?"6":"6"==r?"9":r}var flip="(╯°□°）╯︵ ",table="┻━┻";void 0!==process.argv[1]&&(table=flipString(process.argv[1])),console.log(flip+table);' $1